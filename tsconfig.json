{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "strict": true,
        "sourceMap": true,
        "experimentalDecorators": true,
		"jsx": "react",
        "outDir": "dist",
        "lib": [
            "es2015",
            "dom"
        ]
    },
    "exclude": [
        "node_modules", "dist"
    ]
}
